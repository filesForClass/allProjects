package com.lq.exercises;

public class Car {
	
	private int speed;
	private int gasoline;
	private boolean engineState;
	
	public void setSpeed ( int s ) {
		
		speed = s;
	}
	
	public int getSpeed() {
		return speed;
	}
	
	public void setgasoline( int s ) {
		
		gasoline = s;
	}
	
	public int getgasoline() {
		return gasoline;
	}
	
	public void setengineState ( bool s ) {
		
		engineState = s;
	}
	
	public bool getengineState () {
		return engineState;
	}
}